<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ss.tp.model">

<class name="WgPrIncDecOther" table="WG_PR_INC_DEC_OTHER">

     <id name="keySeq"
        type="long"
        column="KEY_SEQ"
        length="8" >
        <generator class="increment" />
    </id>
	<property name="ouCode" type="string" column="OU_CODE" length="3" />
	<property name="yearPr" type="integer" column="YEAR_PR"  />
	<property name="periodPr" type="integer" column="PERIOD_PR"  />
	<property name="empCode" type="string" column="EMP_CODE" length="10"/>
	<property name="groupCode" type="string" column="GROUP_CODE" length ="1" />
	<property name="incDecCode" type="string" column="INC_DEC_CODE" length ="10"/>
	<property name="yearWork" type="integer" column="YEAR_WORK" />
	<property name="periodWork" type="integer" column="PERIOD_WORK" />
	<property name="codeSeq" type="integer" column="CODE_SEQ" />
	<property name="totalAmt" type="double" column="TOTAL_AMT" />
	<property name="flagPr" type="string" column="FLAG_PR" length="1"/>
	<property name="seqData" type="integer" column="SEQ_DATA" />
	<property name="confirmFlag" type="string" column="CONFIRM_FLAG" length="1"/>
	<property name="creBy" type="string" column="CRE_BY" length="15"/>
	<property name="creDate" type="timestamp" column="CRE_DATE" />
	<property name="updBy" type="string" column="UPD_BY" length="15"/>
	<property name="updDate" type="timestamp" column="UPD_DATE" />
	
</class>
</hibernate-mapping>