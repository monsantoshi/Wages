<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ss.tp.model">

<class name="ApTable" table="AP_TABLE">

     <id name="keySeq"
        type="long"
        column="KEY_SEQ"
        length="8" >
        <generator class="increment" />
    </id>
	<property name="ouCode" type="string" column="OU_CODE" length="3" />
	<property name="yearPn" type="double" column="YEAR_PN" length="4" />
	<property name="monthPn" type="double" column="MONTH_PN" length="2" />
	<property name="volumeSet" type="string" column="VOLUME_SET" length="2"/>
	<property name="empCode" type="string" column="EMP_CODE" length="10"/>
	<property name="codeSeq" type="long" column="CODE_SEQ" />
	<property name="newOldDuty" type="string" column="NEW_OLD_DUTY" length="10"/>
	<property name="newPositionCode" type="string" column="NEW_POSITION_CODE" length="10"/>
	<property name="newLevelCode" type="string" column="NEW_LEVEL_CODE" length="10"/>
	<property name="newGworkCode" type="string" column="NEW_GWORK_CODE" length="10"/>
	<property name="newDuty" type="string" column="NEW_DUTY" length="10"/>
	<property name="newOrgCode" type="string" column="NEW_ORG_CODE" length="14"/>
	<property name="newCodeSeq" type="long" column="NEW_CODE_SEQ" />
	<property name="seqData" type="double" column="SEQ_DATA" length="3"/>
	<property name="totAmt" type="double" column="TOT_AMT"/>
	<property name="creBy" type="string" column="CRE_BY" length="15"/>
	<property name="creDate" type="timestamp" column="CRE_DATE" />
	<property name="updBy" type="string" column="UPD_BY" length="15"/>
	<property name="updDate" type="timestamp" column="UPD_DATE" />
	<property name="confirmFlag" type="string" column="CONFIRM_FLAG" length="10"/>
	<property name="approveFlag" type="string" column="APPROVE_FLAG" length="10"/>
	<property name="transferFlag" type="string" column="TRANSFER_FLAG" length="10"/>
	<property name="bankFlag" type="string" column="BANK_FLAG" length="10"/>
	<property name="approveBy" type="string" column="APPROVE_BY" length="15"/>
	<property name="approveDate" type="timestamp" column="APPROVE_DATE" />
	<property name="transferBy" type="string" column="TRANSFER_BY" length="15"/>
	<property name="transferDate" type="timestamp" column="TRANSFER_DATE" />
	<property name="bankBy" type="string" column="BANK_BY" length="15"/>
	<property name="bankDate" type="timestamp" column="BANK_DATE" />
	<property name="approveClose" type="string" column="APPROVE_CLOSED" length="10"/>
	<property name="bankClose" type="string" column="BANK_CLOSED" length="10"/>
	<property name="transferClose" type="string" column="TRANSFER_CLOSED" length="10"/>
	
</class>
</hibernate-mapping>