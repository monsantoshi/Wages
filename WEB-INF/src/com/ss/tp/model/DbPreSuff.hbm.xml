<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ss.tp.model">

<class name="DbPreSuff" table="DB_PRE_SUFF">

    <id name="preSuffCode"
        type="string"
        column="PRE_SUFF_CODE"
        length="10" >
        <generator class="assigned" />
    </id>
	<property name="prefixName" type="string" column="PREFIX_NAME" length="50" />
	<property name="suffixName" type="string" column="SUFFIX_NAME" length="50" />
	<property name="inactive" type="string" column="INACTIVE" length="1" />
</class>
</hibernate-mapping>