<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ss.tp.model">

<class name="VApSumBatch" table="V_AP_SUM_BATCH">
	 <composite-id name="pk" class="VApSumBatchPK">
	            <key-property name="year" column="YEAR" type="string" />
	            <key-property name="month" column="MONTH" type="string" />
	            <key-property name="volume" column="VOLUME" type="string" length="10"/>
	 </composite-id>
	<property name="self" type="string" column="SELF"  />	 
	<property name="oth" type="string" column="OTH"  />
	<property name="total" type="string" column="TOTAL"  />	 	 
	
		
	
</class>
</hibernate-mapping>