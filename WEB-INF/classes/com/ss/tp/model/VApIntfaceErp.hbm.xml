<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ss.tp.model">

<class name="VApIntfaceErp" table="V_AP_INTFACE_ERP">
	 <composite-id name="pk" class="VApIntfaceErpPK">
	            <key-property name="seq" column="SEQ" type="string" length="1" />
	            <key-property name="year" column="YEAR" type="string" />
	            <key-property name="month" column="MONTH" type="string" />
	            <key-property name="account" column="ACCOUNT" type="string" length="10"/>
	 </composite-id>
	<property name="debit" type="string" column="DEBIT"  />	 
	<property name="credit" type="string" column="CREDIT"  />
	<property name="cont" type="string" column="CONT"  />	 	 
	
		
	
</class>
</hibernate-mapping>