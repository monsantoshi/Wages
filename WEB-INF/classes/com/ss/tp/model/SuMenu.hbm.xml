<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">    
    
<hibernate-mapping package="com.ss.tp.model">
<class  name="SuMenu" table="SU_MENU"> 
    <id name="menuId"
        type="string"
        column="MENU_ID"
        length="10" >
        <generator class="assigned" />
    </id> 
  <property name="menuName" type="string" column="MENU_NAME" length="100" />
  <property name="mainMenu" type="string" column="MAIN_MENU" length="10" />
  <property name="linkName" type="string" column="LINK_NAME" length="5" />  
</class>
</hibernate-mapping>